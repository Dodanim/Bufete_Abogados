<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <navigation-case>
            <from-view-id>/clientes/clientes.xhtml</from-view-id>
    
            <from-outcome>cliente</from-outcome>
            <to-view-id>/clientes/nuevoCliente.xhtml</to-view-id>
        </navigation-case>
    
        <navigation-case>
            <from-view-id>/clientes/poderes.xhtml</from-view-id>
    
            <from-outcome>registro</from-outcome>
            <to-view-id>/clientes/nuevoPoder.xhtml</to-view-id>
        </navigation-case>
    
        <navigation-case>
            <from-view-id>/colaboradores/colaborador.xhtml</from-view-id>
        
            <from-outcome>colaborador</from-outcome>
            <to-view-id>/colaboradores/nuevoColaborador.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            
            <from-view-id>/casos/casos.xhtml</from-view-id>
        
            <from-outcome>caso</from-outcome>
            <to-view-id>/casos/nuevoCaso.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-view-id>/casos/casos.xhtml</from-view-id>
        
            <from-outcome>referencia</from-outcome>
            <to-view-id>/casos/bitacora.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-view-id>/casos/bitacora.xhtml</from-view-id>
        
            <from-outcome>listaBitacora</from-outcome>
            <to-view-id>/casos/bitacora.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-view-id>/index.xhtml</from-view-id>
        
            <from-outcome>master_BA</from-outcome>
            <to-view-id>/master_BA.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
